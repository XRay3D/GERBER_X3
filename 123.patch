+    double ky = rect.bottom() * scale;
+    double kx = rect.left() * scale;
+    if(rect.width() > rect.height())
+        ky += (static_cast<double>(IconSize) - rect.height() * scale) / 2;
+    else
+        kx -= (static_cast<double>(IconSize) - rect.width() * scale) / 2;
